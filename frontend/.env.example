# IntelliAttend Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/api
REACT_APP_WEBSOCKET_URL=ws://localhost:8765

# Development Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_DARK_MODE=true

# Map Configuration (if using maps)
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_DEFAULT_LAT=17.3850
REACT_APP_DEFAULT_LNG=78.4867

# App Configuration
REACT_APP_NAME=IntelliAttend
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Intelligent Attendance Management System

# Build Configuration
GENERATE_SOURCEMAP=true
REACT_APP_BUILD_DATE=2024-01-01